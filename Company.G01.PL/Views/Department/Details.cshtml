@model Department
@{
    ViewData["Title"] = "Details";
}

<h1>Details Department</h1>

<br />
<br />

<form>
    <div class="my-3">
        <label asp-for="Code" class="col-from-label"></label>
        <input asp-for="Code" class="form-control" value="@Model.Code" readonly />
    </div>
    <div class="my-3">
        <label asp-for="Name" class="col-from-label"></label>
        <input asp-for="Name" class="form-control" value="@Model.Name" readonly />
    </div>
    <div class="my-3">
        <label asp-for="CreateAt" class="col-from-label"></label>
        <input asp-for="CreateAt" class="form-control"  readonly  />
    </div>

    <div class="my-3"> 
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
        <a asp-action="Index" class="btn btn-light">Back To List</a>
    </div>
</form>

